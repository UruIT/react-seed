{
	"name": "react-seed-app",
	"version": "1.0.0",
	"description": "React Seed App",
	"main": "index.js",
	"scripts": {
		"precommit": "lint-staged",
		"prepush": "npm test",
		"test": "cd client && npm test",
		"tdd": "cd client && npm run tdd",
		"lint": "eslint . --ext .js,.jsx --ignore-path .gitignore --cache",
		"stats": "cd client && npm run stats",
		"build": "cd client && npm run build",
		"start": "cd server && npm start",
		"dev": "cd server && npm run dev",
		"prod": "npm run build && npm start",
		"migrate:make": "cd server && npm run migrate:make",
		"migrate:latest": "cd server && npm run migrate:latest",
		"migrate:rollback": "cd server && npm run migrate:rollback",
		"install:client": "cd client && yarn",
		"install:server": "cd server && yarn",
		"postinstall": "npm run install:client && npm run install:server"
	},
	"lint-staged": {
		"*.{js,jsx}": [
			"eslint"
		]
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/UruIT/wealth-mastery.git"
	},
	"keywords": [],
	"author": "UruIT (https://github.com/UruIT)",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/UruIT/wealth-mastery/issues"
	},
	"homepage": "https://github.com/UruIT/wealth-mastery#readme",
	"dependencies": {
		"prop-types": "^15.5.10",
		"react": "^15.5.4",
		"react-dom": "^15.5.4"
	},
	"devDependencies": {
		"babel-core": "^6.24.1",
		"babel-eslint": "^7.2.3",
		"babel-loader": "^7.0.0",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-react": "^6.24.1",
		"babel-preset-react-hmre": "^1.1.1",
		"babel-preset-survivejs-kanban": "^0.3.3",
		"clean-webpack-plugin": "^0.1.16",
		"css-loader": "^0.28.4",
		"enzyme": "^2.8.2",
		"eslint": "^3.19.0",
		"eslint-loader": "^1.7.1",
		"eslint-plugin-react": "^7.0.1",
		"extract-text-webpack-plugin": "^2.1.0",
		"file-loader": "^0.11.1",
		"husky": "^0.13.4",
		"html-webpack-plugin": "^2.28.0",
		"html-webpack-template": "^6.0.1",
		"istanbul-instrumenter-loader": "^2.0.0",
		"jest": "^20.0.4",
		"json-loader": "^0.5.4",
		"lint-staged": "^3.5.1",
		"node-sass": "^4.5.3",
		"npm-install-webpack-plugin": "^4.0.4",
		"raw-loader": "^0.5.1",
		"react-test-renderer": "^15.5.4",
		"resolve-url-loader": "^2.0.2",
		"sass-loader": "^6.0.5",
		"style-loader": "^0.18.1",
		"webpack": "^2.6.1",
		"webpack-merge": "^4.1.0",
		"webpack-dev-middleware": "^1.10.2",
		"webpack-hot-middleware": "^2.18.0"
	},
	"engines": {
		"node": ">=7"
	}
}
